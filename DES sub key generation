#include <stdio.h>

void generate_subkeys() {
    printf("DES Subkey Generation:\n");
    printf("Each subkey is generated by selecting 24 bits from the left 28-bit half and 24 bits from the right 28-bit half of the initial key.\n");
    printf("These subsets are disjoint and are combined to form a 48-bit subkey for each round.\n");
}

int main() {
    generate_subkeys();
    return 0;
}
